{
    "name": "qobo/cakephp-search",
    "description": "Search plugin for CakePHP",
    "keywords": ["cakephp", "search"],
    "type": "cakephp-plugin",
    "license": "MIT",
    "homepage": "https://www.qobo.biz",
    "authors": [
        {
            "name": "Qobo Ltd",
            "email": "support@qobo.biz",
            "homepage": "https://www.qobo.biz",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/QoboLtd/cakephp-search/issues",
        "source": "https://github.com/QoboLtd/cakephp-search"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1"
        }
    },
    "require": {
        "qobo/cakephp-roles-capabilities": "^18.0",
        "qobo/cakephp-utils": "^13.0"
    },
    "require-dev": {
        "qobo/cakephp-composer-dev": "^v1.0"
    },
    "autoload": {
        "psr-4": {
            "Search\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Search\\Test\\": "tests/",
            "Groups\\Test\\": "vendor/qobo/cakephp-groups/tests/",
            "RolesCapabilities\\Test\\": "vendor/qobo/cakephp-roles-capabilities/tests/",
            "CakeDC\\Users\\Test\\": "vendor/cakedc/users/tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "test": [
            "phpcs",
            "phpunit --no-coverage"
        ],
        "test-coverage": [
            "phpcs",
            "phpunit"
        ],
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "scripts-descriptions": {
        "test": "Runs phpcs and phpunit without coverage",
        "test-coverage": "Runs phpcs and phpunit with coverage enabled"
    },
    "prefer-stable": true
}
